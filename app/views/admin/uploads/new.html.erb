<%= form_for :dump, :url=>{:action=>"create"}, :html => { :multipart => true } do |f| %>
  <table>
      <tr>
      <td>
        <label for="dump_file">
          Select an Account:
        </label>
      </td>
      <td>
        <%= f.select :account_id, options_for_select([["Import from Mint", nil]] + Account.pluck(:name, :id)) %>
      </td>
    </tr>
    <tr>
      <td>
        <label for="dump_file">
          Select a CSV File:
        </label>
      </td>
      <td>
        <%= f.file_field :file %>
      </td>
    </tr>
    <tr>
      <td>
        <%= submit_tag 'Submit' %>
      </td>
    </tr>
  </table>
<% end %>
